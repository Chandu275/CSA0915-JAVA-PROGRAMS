public class specialchar {
    public static void main(String[] args){
        String n="modi birthday @ september 17,#&$% is the whises code for him";
        int count=0;
        for(int i=0;i<n.length();i++){
            char c= n.charAt(i);
            if(c=='@'||c=='#'||c=='$'||c=='%'||c=='&'){
                count++;
            }
        }
        System.out.println("special characters:"+count);
    }
}
